{
  "name": "DevJourney",
  "version": "1.0.0",
  "description": "Landing page of the first website of Junghoon as a developer",
  "main": "script.js",
  "scripts": {
    "watch:sass": "node-sass main.scss css/style.css -w",
    "devserver": "live-server --browser='Google Chrome'",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass main.scss css/style.comp.css",
    "concat:css": "concat css/icon-font.css css/style.comp.css -o css/style.concat.css",
    "prefix:css": "postcss css/style.concat.css -o css/style.prefix.css --use autoprefixer -b 'last 10 versions'",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Junghoon Suk",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.2.0"
  }
}
